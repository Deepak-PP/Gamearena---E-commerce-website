    <!-- CONTENT -->
    <section id="content">
      <!-- NAVBAR -->
     
      <!-- NAVBAR -->
    
      <!-- MAIN -->
      <main>
        <div class="head-title">
          <div class="left">
            <h1 class="text-danger">Orders</h1>
           
          </div>
    
        </div>
    
    
    
        <!-- <div class="container py-5"> -->
    
        <div class="" style="border-radius: 20px; background-color: #f9f9f9;">
          <div class="row container-fluid  ">
    
            <div class="col-lg-12 ">
              <div class="card mb-4 mt-3 me-4 ">
                <form class="card-body" action="/admin/orderUpdate" method="post">
    
                  <div class="row">
                    <div class="col-sm-3">
                      <p class="mb-0 ">OrderID</p>
                    </div>
                    <div class="col-sm-9">
                      <p>
                        <%= orderData._id %>
                      </p>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-sm-3">
                      <p class="mb-0 ">Username</p>
                    </div>
                    <div class="col-sm-9">
                      <p>
                        <%= orderData.userId.name %>
                      </p>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-sm-3">
                      <p class="mb-0 ">Products</p>
                    </div>
                    <div class="col-sm-9 ">
                      <%for(let i=0;i<orderData.products.length;i++){%>
                        <div class="d-flex flex-row">
                          <img src="/productImages/<%= orderData.products[i].item[0].image[0] %>" width="100px"
                            height="100px" alt="">
                          <p>
                            <%= orderData.products[i].item[0].name%><br><span class="text-muted ">qty:</span>
                              <%=orderData.products[i].quantity%>
                          </p>
    
                        </div>
    
    
                        <%}%>
    
                    </div>
                  </div>
    
                  <div class="row pt-4">
                    <div class="col-sm-3">
                      <p class="mb-0">Total</p>
                    </div>
                    <div class="col-sm-9">
                      <p>â‚¹<%= orderData.total %>
                      </p>
                    </div>
                  </div>
    
                  <div class="row pt-4">
                    <div class="col-sm-3">
                      <p class="mb-0">Date</p>
                    </div>
                    <div class="col-sm-9">
                      <p>
                        <%= orderData.createdAt.toString().slice(0,16) %>
                      </p>
                    </div>
                  </div>
    
                  <div class="row pt-4">
                    <div class="col-sm-3">
                      <p class="mb-0">Time</p>
                    </div>
                    <div class="col-sm-9 ">
                      <p>
                        <%= orderData.createdAt.toString().slice(16,24) %>
                      </p>
                    </div>
                  </div>
                  <div class="row pt-4">
                    <div class="col-sm-3">
                      <p class="mb-0">Payment Method</p>
                    </div>
                    <div class="col-sm-9 ">
                      <p>
                        <%= orderData.payment_method %>
                      </p>
                    </div>
                  </div>
    
                  <div class="row pt-4">
                    <div class="col-sm-3">
                      <p class="mb-0">Payment Status</p>
                    </div>
                    <div class="col-sm-9 ">
                      <p>
                        <%= orderData.payment_status %>
                      </p>
                    </div>
                  </div>
                  <div class="row pt-4">
                    <div class="col-sm-3">
                      <p class="mb-0">Order Status</p>
                    </div>
                    <!-- <div class="dropdown">
                        <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown"  aria-expanded="false">
                          Pending
                        </button>
                        <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1" na>
                          <li><a class="dropdown-item" href="#">Pending</a></li>
                          <li><a class="dropdown-item" href="#">Placed</a></li>
                          <li><a class="dropdown-item" href="#">Shipped</a></li>
                          <li><a class="dropdown-item" href="#">Out for Delivery</a></li>
                          <li><a class="dropdown-item" href="#">Delivered</a></li>
                          <li><a class="dropdown-item" href="#">Cancelled</a></li>
                        </ul>
                      </div> -->
                    <select name="order_status" id="" style="border: none; width:auto">
                      <option>
                        <%=orderData.order_status%>
                      </option>
                      <option>Placed</option>
                      <option>Shipped</option>
                      <option>Out for Delivery</option>
                      <option>Delivered</option>
                      <option>Cancelled</option>
                    </select>
                  </div>
    
                  <div class="row pt-4">
                    <div class="col-sm-3">
                      <p class="mb-0">Address</p>
                    </div>
                    <div class="col-sm-9">
                      <p class="text-muted mb-0 ">
                       <%=address.name%>,
                      <%=address.address1%>,<br>
                      <%=address.mobile%>
                     

                      </p>
                    </div>
                  </div>
              </div>
              <input type="hidden" name="id" value="<%=orderData._id%>">
              <button type="submit" class="btn btn-outline-success">Apply Changes</button>
              </form>
    
    
      </main>
      <!-- MAIN -->
    </section>


    <script src="/jquery-3.3.1.min.js"></script>
    <!-- https://jquery.com/download/ -->
    <script src="/jquery-ui.min.js"></script>
    <!-- https://jqueryui.com/download/ -->
    <script src="/bootstrap.min.js"></script>
    
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
    
    <script src="https://kit.fontawesome.com/d4801b7ff5.js" crossorigin="anonymous"></script>
    <!-- CONTENT -->
  </body>
