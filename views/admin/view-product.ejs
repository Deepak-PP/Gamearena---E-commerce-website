    <div class="container tm-mt-big tm-mb-big">
      <div class="row">
        <div class="col-xl-9 col-lg-10 col-md-12 col-sm-12 mx-auto">
          <div class="tm-bg-primary-dark tm-block tm-block-h-auto">
            <div class="row">
              <div class="col-12">
                <h2 class="tm-block-title d-inline-block">USER DETAILS</h2>
              </div>
            </div>
          
            <form action="/admin/add-product" method="POST" class="tm-edit-product-form">
            <div class="row tm-edit-product-row">
              <div class="col-xl-6 col-lg-6 col-md-12">
                
                  <div class="form-group mb-3">
                    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                      <div class="carousel-inner">
                        <div class="carousel-item active">
                          <img class="d-block w-100" src="/productImages/<%= products.image[0] %>" alt="First slide">
                        </div>
                        <div class="carousel-item">
                          <img class="d-block w-100" src="/productImages/<%= products.image[1] %>" alt="Second slide">
                        </div>
                        <div class="carousel-item">
                          <img class="d-block w-100" src="/productImages/<%= products.image[2] %>" alt="Third slide">
                        </div>
                      </div>
                      <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                      </a>
                      <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                      </a>
                    </div>
                    <label
                      for="name"
                      >Product name-
                    </label>
                  <p> <%=products.name%></p>
                  </div>

                  <div class="form-group mb-3">
                    <label
                      for="name"
                      >Price-
                    </label>
                  <p><%=products.price%></p>
                  </div>
                  <div class="form-group mb-3">
                    <label
                      for="name"
                      >Connectivity-
                    </label>
                  <p><%=products.connectivity%></p>
                  </div>

                  <div class="form-group mb-3">
                    <label
                      for="name"
                      >Category-
                    </label>
                  <p> <%= products.category.name %></p>
                  </div>

                  <!-- <div class="form-group mb-3">
                    <label
                      for="name"
                      >hdd
                    </label>
                  <p> <%=products.hdd%></p>
                  </div> -->
                 
                  </div>
                  
              </div>
              
              <!-- <div class="col-12">
                <button type="submit" class="btn btn-primary btn-block text-uppercase">Add Product Now</button>
              </div> -->
            </form>
           
            </div>
          </div>
